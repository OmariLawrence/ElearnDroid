<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.olawr.elearndroid2018">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Topics"
            android:label="@string/title_activity_topics"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TopicDetail"
            android:label="@string/title_activity_topic_detail" />
        <activity
            android:name=".Quiz"
            android:label="@string/title_activity_quiz" />
        <activity android:name=".calculate" />
        <activity
            android:name=".paperselect"
            android:label="@string/title_activity_paper_select"
            android:screenOrientation="portrait" />
        <activity android:name=".paper" />
        <activity android:name=".aboutandroid"></activity>
    </application>

</manifest>